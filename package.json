{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend": "vite",
    "start": "concurrently -n 'backend,frontend' -c  \"red,blue\" \"nodemon  server/app\" \"npm run frontend\"",
    "build": "tsc && vite build",
    "preview": "vite preview 5000 --strictPort",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "prepare": "husky",
    "server": "nodemon server/app.js",
    "translate": "node src/locales/generateTranslations.js"
  },
  "lint-staged": {
    "*.{ts, tsx, js}": [
      "eslint"
    ],
    "*.{scss, css}": [
      "stylelint"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@popperjs/core": "^2.11.8",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "react": "^19.2.4",
    "react-compiler-healthcheck": "^1.0.0",
    "react-day-picker": "^9.13.1",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.13.0",
    "react-select": "^5.10.2",
    "vite-plugin-babel": "^1.4.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "babel-plugin-react-compiler": "^1.0.0",
    "bcryptjs": "^3.0.3",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsonwebtoken": "^9.0.3",
    "lint-staged": "^16.2.7",
    "mongoose": "^9.1.6",
    "multer": "^2.0.2",
    "nodemon": "^3.1.11",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "stylelint": "^17.1.1",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-eslint2": "^5.0.4"
  }
}
